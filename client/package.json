{
  "name": "quizzy-rascal-client",
  "version": "0.0.1",
  "description": "Quizzy Rascal client application",
  "main": "app.js",
  "engines": {
    "node": "4.2.3"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "npm run build -- --watch --progress",
    "lint": "eslint --ext .js src",
    "test": "mocha --compilers js:babel-core/register src/**/*-spec.js",
    "test:watch": "npm run test -- --watch --reporter dot --growl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simoncrabtree/quizzy-rascal.git"
  },
  "dependencies": {
    "babel-core": "6.5.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.3",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "bootstrap": "3.3.6",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.0.0",
    "file-loader": "0.8.5",
    "highcharts": "4.2.3",
    "isomorphic-fetch": "2.2.1",
    "jquery": "2.2.0",
    "less": "2.6.0",
    "less-loader": "2.2.2",
    "moment": "2.11.2",
    "node-libs-browser": "1.0.0",
    "object-assign": "4.0.1",
    "raw-loader": "0.5.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-highcharts": "8.0.2",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "redux": "3.3.1",
    "redux-logger": "2.5.2",
    "redux-thunk": "1.0.3",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "uuid": "2.0.1",
    "webpack": "1.12.13"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "deep-freeze": "0.0.1",
    "growl": "1.9.2",
    "mocha": "2.4.5"
  }
}
